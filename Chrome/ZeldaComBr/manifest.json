{
	"name": "Zelda.com.br",
	"description": "Site n\u00e3o-oficial sobre a s\u00e9rie The Legend of Zelda, feito de f\u00e3s para f\u00e3s!",
	"version": "1.0.3",
	"manifest_version": 2,
	"permissions": [ "http://apps.aloogle.net/blogapp/*", "http://*.zelda.com.br/*", "storage", "notifications" ],
	"options_page": "paginas/opcoes.html",
	"background": {
	"page":
		"background.html"
	},
	"icons": {
		"16": "icons/icon_16.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"omnibox": { 
		"keyword" : "zcb" 
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Shift+Alt+Z"
			}
		}
	},
	"browser_action": {
		"default_icon": {
            "19": "icons/icon.png",
            "38": "icons/icon_38.png"
        },
		"default_title": "Zelda.com.br",
		"default_popup": "popup.html"
	}
}
